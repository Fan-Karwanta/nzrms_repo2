{
  "expo": {
    "jsEngine": "hermes",
    "name": "NZ RMS",
    "version": "3.9.0",
    "slug": "nzrms",
    "icon": "./assets/icon3.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#0D63C9",
      "image": "./assets/icon3.png"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon3.png"
      },
      "icon": "./assets/icon3.png",
      // "splash": {
      //   "backgroundColor": "#0D63C9",
      //   "image": "./assets/splash.png"
      // },
      "googleServicesFile": "./google-services.json",
      "package": "com.movinin"
    },
    "ios": {
      "buildNumber": "3.9.0",
      "supportsTablet": true,
      "icon": "./assets/icon3.png",
      // "splash": {
      //   "backgroundColor": "#0D63C9",
      //   "image": "./assets/splash.png",
      //   "tabletImage": "./assets/splash-tablet.png"
      // },
      "bundleIdentifier": "com.movinin"
    },
    "plugins": [
      ["expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "Allow NZ RMS to use your location."
      }
    ],

      "expo-localization",
      "./plugins/usesCleartextTraffic",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#0D63C9",
          "image": "./assets/icon3.png",
          "imageWidth": 200
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "MERCHANT_IDENTIFIER",
          "enableGooglePay": true
        }
      ]
    ],
    "owner": "aelassas",
    "extra": {
      "eas": {
        "projectId": "700d84eb-8776-420c-8b2f-351287408ba4"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/68f84550-e978-45b5-9de1-da514eb547fe"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
